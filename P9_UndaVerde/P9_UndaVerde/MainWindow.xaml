    <Grid VerticalAlignment="Top" x:Name="main">

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="6*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>

        <Fluent:Ribbon Grid.Row="0">
            <!--Backstage-->
            <Fluent:Ribbon.Menu>
                <Fluent:Backstage>
                    <Fluent:BackstageTabControl>
                        <Fluent:BackstageTabItem Header="About" >
                            <GroupBox Header="Many thanks to those hard working and intelligent students that deserve a 10 :)">
                                <!-- TODO -->
                            </GroupBox>
                        </Fluent:BackstageTabItem>
                    </Fluent:BackstageTabControl>
                </Fluent:Backstage>
            </Fluent:Ribbon.Menu>

            <!--Tabs-->
            <Fluent:RibbonTabItem Header="Main Settings">
                <Fluent:RibbonGroupBox Header="Group">
                    <Fluent:Button Header="Green"/>
                    <Fluent:Button Header="Grey" />
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>

            <Fluent:RibbonTabItem Header="Other Settings">
                <Fluent:RibbonGroupBox Header="OtherGroup">
                    <Button Margin="15" Opacity="0.8" Content="Just a button"/>
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem Header="Another Other Settings">
